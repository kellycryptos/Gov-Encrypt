{
    "version": "0.1.0",
    "name": "gov_encrypt",
    "instructions": [
        {
            "name": "initializeDao",
            "accounts": [
                {
                    "name": "daoState",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "authority",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "quorumThreshold",
                    "type": "u64"
                }
            ]
        },
        {
            "name": "createProposal",
            "accounts": [
                {
                    "name": "daoState",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "proposal",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "creator",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "title",
                    "type": "string"
                },
                {
                    "name": "description",
                    "type": "string"
                },
                {
                    "name": "deadline",
                    "type": "i64"
                }
            ]
        },
        {
            "name": "delegateVote",
            "accounts": [
                {
                    "name": "delegation",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "delegator",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "delegate",
                    "type": "publicKey"
                },
                {
                    "name": "weight",
                    "type": "u64"
                }
            ]
        },
        {
            "name": "submitEncryptedVote",
            "accounts": [
                {
                    "name": "encryptedVote",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "proposal",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "voter",
                    "isMut": true,
                    "isSigner": true
                },
                {
                    "name": "systemProgram",
                    "isMut": false,
                    "isSigner": false
                }
            ],
            "args": [
                {
                    "name": "encryptedBlob",
                    "type": "bytes"
                }
            ]
        },
        {
            "name": "vote",
            "accounts": [
                {
                    "name": "proposal",
                    "isMut": true,
                    "isSigner": false
                },
                {
                    "name": "daoState",
                    "isMut": false,
                    "isSigner": false
                },
                {
                    "name": "mpcNode",
                    "isMut": true,
                    "isSigner": true
                }
            ],
            "args": [
                {
                    "name": "voteChoice",
                    "type": "u8"
                },
                {
                    "name": "weight",
                    "type": "u64"
                }
            ]
        }
    ],
    "accounts": [
        {
            "name": "DaoAccount",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "authority",
                        "type": "publicKey"
                    },
                    {
                        "name": "authorizedMpcNode",
                        "type": "publicKey"
                    },
                    {
                        "name": "quorumThreshold",
                        "type": "u64"
                    },
                    {
                        "name": "proposalCount",
                        "type": "u64"
                    }
                ]
            }
        },
        {
            "name": "Proposal",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "id",
                        "type": "u64"
                    },
                    {
                        "name": "creator",
                        "type": "publicKey"
                    },
                    {
                        "name": "title",
                        "type": "string"
                    },
                    {
                        "name": "description",
                        "type": "string"
                    },
                    {
                        "name": "deadline",
                        "type": "i64"
                    },
                    {
                        "name": "yesVotes",
                        "type": "u64"
                    },
                    {
                        "name": "noVotes",
                        "type": "u64"
                    },
                    {
                        "name": "status",
                        "type": {
                            "defined": "ProposalStatus"
                        }
                    }
                ]
            }
        },
        {
            "name": "Delegation",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "delegator",
                        "type": "publicKey"
                    },
                    {
                        "name": "delegate",
                        "type": "publicKey"
                    },
                    {
                        "name": "weight",
                        "type": "u64"
                    }
                ]
            }
        },
        {
            "name": "EncryptedVote",
            "type": {
                "kind": "struct",
                "fields": [
                    {
                        "name": "voter",
                        "type": "publicKey"
                    },
                    {
                        "name": "proposalId",
                        "type": "u64"
                    },
                    {
                        "name": "encryptedBlob",
                        "type": "bytes"
                    },
                    {
                        "name": "timestamp",
                        "type": "i64"
                    }
                ]
            }
        }
    ],
    "types": [
        {
            "name": "ProposalStatus",
            "type": {
                "kind": "enum",
                "variants": [
                    {
                        "name": "Active"
                    },
                    {
                        "name": "Passed"
                    },
                    {
                        "name": "Failed"
                    }
                ]
            }
        }
    ],
    "errors": [
        {
            "code": 6000,
            "name": "VotingClosed",
            "msg": "Voting period is closed"
        },
        {
            "code": 6001,
            "name": "InvalidVoteChoice",
            "msg": "Invalid vote choice"
        },
        {
            "code": 6002,
            "name": "UnauthorizedNode",
            "msg": "Unauthorized MPC node"
        }
    ]
}